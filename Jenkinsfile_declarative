pipeline {
  agent any
  stages {
    stage('Source') {
      steps {
        git clone 'https://github.com/digitalvarys/jenkins-tutorials.git'
      }
    }
    stage('Compile') {
      tools {
        gradle 'gradle4'
      }
      steps {
        sh 'gradle clean compileJava test'
      }
    }
  }
}
